# list slicing
myList=[20,23,89,90,12]
#[start:end]
print("List slicing",myList[2:4]) #89,90, go to end-1 index 
numbers = [10, 20, 30, 40, 50]

# it will go to end-1 mean 2index 
print(numbers[:3])
print("Till end :",numbers[2:])



#---------------------------
# negative Slicing
numbers = [10, 20, 30, 40, 50]
print("Original list:",numbers)
print(numbers[-4:-1])
# it will also go to end-1 index 
# 50 mean -1 index out put will be 20,30,40,50

numbers = [1, 2, 3, 4, 5]
print(numbers[::-1])

a =["Abc","def"]
b= a[:]
b.append(2)
print("Copy List:",b)
numbers1 = [1, 2, 3, 4, 5]

numbers1[1:4] = [20, 30, 40]
print(numbers1)
# delete function
numbers2 = [1, 2, 3, 4, 5, 6]

# del keyword we will also use at index level or slice 
del numbers2[2:5]
print(numbers2)

l=[2,3,4]
l2=[5,6,7]
l.extend(l2)
print("After extended list:",l)
# l.clear(),delete all element or give empty string




#-------------------------------------------
#packing or unpacking

# python automatic pack values into tuple
def add_numbers(*nums):
    print(nums)

add_numbers(1, 2, 3, 4)

#unpacking , is extracting values,stored in variables

data=(10,20,30)
a,b,c=data
print("unpacking value",a)
print("unpacking value",b)
print("unpacking value",c)

# unpacking list:
numbers = [1, 2, 3]

x, y, z = numbers
print(x, y, z)
# if we donot wnt to store values, use'-'
n1 = [1, 2, 3]

x, _, z = n1
print("original list:",n1)
print("Ignoring middle value",x,z)

