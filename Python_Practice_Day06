#stack implemention
class Stack:
    def __init__(self):
        self.items=[]
        #list of elements
    def isEmpty(self):
       return len(self.items)==0
    def push(self,item):
        self.items.append(item)
        print("pushed item:",item)
    def pop(self):
        if self.isEmpty():
            print("stack underflow")
            return None          
        return self.items.pop()
    def peek(self):
         if self.isEmpty():
            print("Empty Stack")
            return None  
         return self.items[-1]
    def size(self):
        return len(self.items)
    def print(self):
        print("items  :",self.items) 

stack = Stack()
stack.push(10)
stack.push(20)
stack.push(30)
stack.print()          
print("Top element:", stack.peek())   
print("Popped:", stack.pop())       
stack.print()                      #
print("Is stack empty?", stack.isEmpty())  # False
print("Stack size:", stack.size())          
                
#_____________________________

# Queue Implementaion


class Queue:
    def __init__(self):
        self.items = []  
    def isEmpty(self):
        return len(self.items) == 0
    def enqueue(self, item):
        self.items.append(item)
        print("Items:",item)

    def dequeue(self):
        if self.isEmpty():
            print("Queue Underflow")
            return None
        return self.items.pop(0)

    def peek(self):
        if self.isEmpty():
            print("Queue is empty")
            return None
        return self.items[0]

    def size(self):
        return len(self.items)

  
    def print(self):
        print("Queue ", self.items)

q = Queue()
q.enqueue(10)
q.enqueue(20)
q.enqueue(30)

q.print()               
print("Front element:", q.peek()) 
print("Dequeued:", q.dequeue())   
q.print()                        
print("Is queue empty?", q.isEmpty()) 
print("Queue size:", q.size())          

