print("maxsplit",l)

#__________________________________________
# strip()
# strip() function work only for string and also return a string
text="%%%%Wajeeha Yaseen****"
t=text.strip("%")
print("after strip",t)

text2="%%%%Wajeeha Yaseen****"
t2=text2.strip("*")
print("after strip",t2)

fruit=",,,,,apple,banana, orange,,,,,,,"
f=fruit.strip(",,,,,")
print("after stripping",f)
print("last word:",f[-1])


