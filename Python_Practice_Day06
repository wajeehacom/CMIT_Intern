#RegEx
import re

text="My Name is Wajeeha Yaseen"
t=re.findall("\AMy",text)
print(t)
if t:
    print("Match")
else:
 print("Not found")   

#RegEx using some special Character
#find digits:
d="Wajeeha Yaseen 283 **Lahore 283"
print(" Digits :",re.findall(r'\d',d))
# non digits
print(" Non digits :",re.findall(r'\D',d))
# word character
print(" word character :",re.findall(r'\w',d))
# non word character
print(" Non word character :",re.findall(r'\W',d))
# whitespace
print("whiteSpace character :",re.findall(r'\s',d))
# nonwhitespace
print(" Non WhiteSpace :",re.findall(r'\S',d))
text1 = "Hello World"
print(re.findall(r'\Bell\B', text1)) 


#------------------------------------
# keyword arguments vs keyword only arguments
def student_detail(name,rollno):
    print("Name:",name,"Age:",rollno)


 
student_detail("Wajeeha Yaseen","BSEF21M042")   #postional arguments

# keyword only Arguments:
def student_detail(name,*,rollno):
    print("Name:",name,"Age:",rollno)


  # learningg points:
  #keyword arguments(pass by name : option hota hai )
 # not necessary to put * in function definition
 # if we student_detail(name,rollno): & pass arguments by name, it will also work
 #student_detail("Wajeeha Yaseen",rollno="BSEF21M042")  withhout name also will run this 
 # this is the main difference both keywrd only or not



  # learningg points:
  #keyword only arguments(mandatory hai)
  #student_detail(name,*,rollno) necessary syntax

 # here we pass arguments by name not by position
 # after putting steric ,all parameters are considered keyword arguments
student_detail("Wajeeha Yaseen",rollno="BSEF21M042") 

# example of variable no of arguements
def order(*items,time):
    print("items :",items,"Time :",time)


# here in this example ,time become a keyword argumet,and item is a postional argument
order(["Orange","banana","Grapes"],time="5AM") 

#_________________________________
# postional vs postional only keyword


# positional arguments are normal function
# same order pass or set value in argument or goes to in parameters
#we pass arguments by name also

# example

def student(name,age):
    print("Name:",name,"Age:",age)

 #it will allso work or  run
student("Wajeeha Yaseen",24)   
#  we can also pass by name
student("Maria Yasin",age=23) 

# postional only keyword
# in whihc we cannot pass arguments by name
# using / , befor this / all parameters are considered postional only keyword
def student(name,age,/):
    print("Name:",name,"Age:",age)

 # name, age here are position only keyword,
student("Saqlain",26)    


#-----------------------------------------

# arbitray arguments

# if single steric * use in function parameter 
# will be postionall arbitray ,accept the variable no.of arguments

def add(*args):
    total=0
    for i in args:
        total+=i
    return total    

print("positional arbitrary argument",add(2,3))
print("positional arbitrary argument",add(2,3,5))



#__________________________
#arbitray keyword arguments

# here **args indicate the arbitray keyword arguments
# pass by name
# in loop ,items() is a dictionary built in function
def add(**args):
    for key,value in args.items():
        print(f"{key}: {value}")

add(Name="Wajeeha Yaseen",Age=21 ,Country="Lahore")


#----------------------#
#Exceptional Hnadling

# try,except,else,finall(always run wether error or not)

#exmple 01


try:
    with open("demo.txt",'r') as file:
        print(file.read())
except FileNotFoundError:
    print("File Not found")
else:
    print("File found")
finally:
     print("Program Ended")   

     


#example02
try:
    x=int(input("Enter Number:"))
    print("Division:",10/x)
except ValueError:
    print("Invalid Number")
except ZeroDivisionError:
    print("Cannot divide by Zero")
else:
    print("Successful ")

finally:
    print("Program ended")   


# raise keyword
age=int(input("Enter Your age:"))
if age<18:
    raise ValueError("Age must be 18 or above")
    #raise.Exception("xyz")
print("Eligible")


